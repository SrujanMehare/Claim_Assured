<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ConsumerButton">
        <attr name="cb_shadow_color" format="color" />
        <attr name="is_primary" format="boolean" />
        <attr name="elevation" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ConsumerOtpEditText">
        <attr name="preventPasswordToggleOverlap" format="boolean" />
    </declare-styleable>


    <declare-styleable name="Pinview">
        <attr name="pinBackground" format="reference" />
        <attr name="pinLength" format="integer" />
        <attr name="pinWidth" format="dimension" />
        <attr name="pinHeight" format="dimension" />
        <attr name="splitWidth" format="dimension" />
        <attr name="cursorVisible" format="boolean" />
        <attr name="password" format="boolean" />
        <attr name="forceKeyboard" format="boolean" />
        <attr name="hint" format="string" />
        <attr name="inputType" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>
    <attr name="inputType" format="enum">
        <enum name="text" value="0" />
        <enum name="number" value="1" />
    </attr>

    <declare-styleable name="CustomLabeledSwitch">
        <attr name="textOn" format="string" />
        <attr name="textOff" format="string" />
        <attr name="android:fontFamily" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="isOn" format="boolean" />
        <attr name="android:enabled" />
    </declare-styleable>


    <declare-styleable name="SwipeButton">
        <attr name="inner_text" format="string"/>
        <attr name="inner_text_color" format="color"/>
        <attr name="inner_text_size" format="dimension"/>
        <attr name="inner_text_left_padding" format="dimension"/>
        <attr name="inner_text_right_padding" format="dimension"/>
        <attr name="inner_text_top_padding" format="dimension"/>
        <attr name="inner_text_bottom_padding" format="dimension"/>
        <attr name="inner_text_background" format="reference"/>
        <attr name="button_image_width" format="dimension"/>
        <attr name="button_image_height" format="dimension"/>
        <attr name="button_trail_enabled" format="boolean"/>
        <attr name="button_trail_drawable" format="reference"/>
        <attr name="button_image_disabled" format="reference"/>
        <attr name="button_image_enabled" format="reference"/>
        <attr name="button_left_padding" format="dimension"/>
        <attr name="button_right_padding" format="dimension"/>
        <attr name="button_top_padding" format="dimension"/>
        <attr name="button_bottom_padding" format="dimension"/>
        <attr name="button_background" format="reference"/>
        <attr name="has_activate_state" format="boolean"/>
        <attr name="initial_state" format="enum">
            <enum name="enabled" value="0"/>
            <enum name="disabled" value="1"/>
        </attr>


    </declare-styleable>

    <declare-styleable name="ClaimAssuredDateTimeEditText">
        <attr name="endIcon" format="reference" />
        <!--<attr name="isDate" format="boolean" />
        <attr name="isTime" format="boolean" />-->
    </declare-styleable>

    <declare-styleable name="CustomSpinnerView">
        <attr name="customBackground" format="reference" />
        <attr name="spinnerEntries" format="reference" />
    </declare-styleable>

    <declare-styleable name="ClaimAssuredLabelEditText">
        <attr name="isMandatoryLabel" format="boolean" />
        <attr name="showLabel" format="boolean" />
        <attr name="labelText" format="string"/>
        <attr name="hintText" format="string"/>
        <attr name="isDate" format="boolean" />
        <attr name="isTime" format="boolean" />
        <attr name="editTextType" format="enum">
            <enum name="standard" value="0" />
            <enum name="phone" value="1" />
            <enum name="dateTime" value="2" />
            <enum name="spinner" value="3" />
        </attr>
    </declare-styleable>


</resources>